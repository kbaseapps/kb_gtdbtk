{
    "ver": "0.1.7",
    "authors": [
        "donovan_parks", "aaronmussig", "pchaumeil", "psdehal", "dylan"
    ],
    "contact": "http://www.kbase.us/support/",
    "categories": ["active", "communities", "annotation"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "input_object_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseGenomes.ContigSet",
                                   "KBaseGenomeAnnotations.Assembly",
                                   "KBaseGenomes.Genome",
                                   "KBaseSets.GenomeSet",
                                   "KBaseSearch.GenomeSet",
                                   "KBaseSets.AssemblySet",
                                   "KBaseMetagenomes.BinnedContigs" ]
            }
        },
        {
            "id": "min_perc_aa",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": ["10"],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int": 1,
                "max_int": 100
            }
        },
        {
            "id": "overwrite_tax",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ 0 ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": 0,
                        "display": "DON'T overwrite Taxonomy",
                        "id": "overwrite_tax-NO",
                        "ui-name": "overwrite_tax-NO"
                    },
                    {
                        "value": 1,
                        "display": "DO overwrite Taxonomy",
                        "id": "overwrite_tax-YES",
                        "ui-name": "overwrite_tax-YES"
                    }
                ]
            }
        }	
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_gtdbtk",
            "method": "run_kb_gtdbtk_classify_wf",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "input_object_ref",
                    "target_property": "input_object_ref",
                    "target_type_transform":"ref"
                },{
                    "input_parameter": "min_perc_aa",
                    "target_property": "min_perc_aa"
                },{
                    "input_parameter": "overwrite_tax",
                    "target_property": "overwrite_tax"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
