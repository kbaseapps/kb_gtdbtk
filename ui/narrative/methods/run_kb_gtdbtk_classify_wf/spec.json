{
    "ver": "1.3.0",
    "authors": [
        "donovan_parks", "aaronmussig", "pchaumeil", "psdehal", "dylan"
    ],
    "contact": "http://www.kbase.us/support/",
    "categories": ["active", "communities", "annotation", "phylogenetics"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
            "id": "input_object_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseSets.GenomeSet",
                                   "KBaseSearch.GenomeSet",
                                   "KBaseSets.AssemblySet",
                                   "KBaseMetagenomes.BinnedContigs" ]
            }
        },
        {
            "id": "output_tree_basename",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "GTDB_Tree" ],
            "field_type": "text",
            "text_options": {
                "is_output_name": true,
                "valid_ws_types": ["KBaseTrees.Tree"]
            }
        },
	{
            "id": "copy_proximals",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ 0 ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": 0,
                        "display": "DON'T copy GTDB proximal genomes",
                        "id": "copy_proximals-NO",
                        "ui-name": "copy_proximals-NO"
                    },
                    {
                        "value": 1,
                        "display": "DO copy GTDB proximal genomes",
                        "id": "copy_proximals-YES",
                        "ui-name": "copy_proximals-YES"
                    }
                ]
            }
        },	
        {
            "id": "save_trees",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ 0 ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": 0,
                        "display": "DON'T save Trees and copy GTDB tree genomes",
                        "id": "save_trees-NO",
                        "ui-name": "save_trees-NO"
                    },
                    {
                        "value": 1,
                        "display": "DO save Trees and copy GTDB tree genomes",
                        "id": "save_trees-YES",
                        "ui-name": "save_trees-YES"
                    }
                ]
            }
        },	
        {
            "id": "min_perc_aa",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": ["10"],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_int": 1,
                "max_int": 100
            }
        },
        {
            "id": "full_tree",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ 0 ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": 0,
                        "display": "use class-level subtrees",
                        "id": "full_tree-NO",
                        "ui-name": "full_tree-NO"
                    },
                    {
                        "value": 1,
                        "display": "use full tree",
                        "id": "full_tree-YES",
                        "ui-name": "full_tree-YES"
                    }
                ]
            }
        },	
        {
            "id": "keep_intermediates",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ 0 ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": 0,
                        "display": "DON'T keep intermediate files",
                        "id": "keep_intermediates-NO",
                        "ui-name": "keep_intermediates-NO"
                    },
                    {
                        "value": 1,
                        "display": "DO keep intermediate files",
                        "id": "keep_intermediates-YES",
                        "ui-name": "keep_intermediates-YES"
                    }
                ]
            }
        },	
        {
            "id": "overwrite_tax",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ 0 ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": 0,
                        "display": "DON'T overwrite Taxonomy",
                        "id": "overwrite_tax-NO",
                        "ui-name": "overwrite_tax-NO"
                    },
                    {
                        "value": 1,
                        "display": "DO overwrite Taxonomy",
                        "id": "overwrite_tax-YES",
                        "ui-name": "overwrite_tax-YES"
                    }
                ]
            }
        }	
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_gtdbtk",
            "method": "run_kb_gtdbtk_classify_wf",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "input_object_ref",
                    "target_property": "input_object_ref",
                    "target_type_transform":"ref"
                },{
                    "input_parameter": "output_tree_basename",
                    "target_property": "output_tree_basename"
                },{
                    "input_parameter": "copy_proximals",
                    "target_property": "copy_proximals"
                },{
                    "input_parameter": "save_trees",
                    "target_property": "save_trees"
                },{
                    "input_parameter": "min_perc_aa",
                    "target_property": "min_perc_aa"
                },{
                    "input_parameter": "full_tree",
                    "target_property": "full_tree"
                },{
                    "input_parameter": "keep_intermediates",
                    "target_property": "keep_intermediates"
                },{
                    "input_parameter": "overwrite_tax",
                    "target_property": "overwrite_tax"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
